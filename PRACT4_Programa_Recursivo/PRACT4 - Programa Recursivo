# Programa Recursivo

import time

def fibonacci_recursivo(n):
    if n <= 1:
        return n
    return fibonacci_recursivo(n-1) + fibonacci_recursivo(n-2)

def fibonacci_iterativo(n):
    if n <= 1:
        return n
    a, b = 0, 1
    for _ in range(2, n + 1):
        a, b = b, a + b
    return b

n = int(input("Ingresa un nÃºmero: "))

inicio_rec = time.time()
resultado_rec = fibonacci_recursivo(n)
fin_rec = time.time()

inicio_it = time.time()
resultado_it = fibonacci_iterativo(n)
fin_it = time.time()

print("\nResultado Recursivo:", resultado_rec)
print("Tiempo Recursivo:", fin_rec - inicio_rec, "segundos")

print("\nResultado Iterativo:", resultado_it)
print("Tiempo Iterativo:", fin_it - inicio_it, "segundos")
